  <h1>Add Bookmark</h1>
    <%= simple_form_for [@list, @bookmark] do |f|%>
    <div class="search-form-control form-group">
    <%= f.input :comment, class: "form-control string required" %>
    <%= f.association :movie, collection: Movie.all, class: "form-control string required" %>
    <button name="button" type="submit" class="btn btn-flat">
      <i class="fas fa"></i> Create
    </button>
    </div>
  <% end %>
